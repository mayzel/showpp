{
  "name": "showpp",
  "displayName": "showpp",
  "description": "Visualize Bruker TopSpin pulse sequences",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:showpp.openPulseSequenceViewer",
    "onCommand:showpp.closePulseSequenceViewer"
  ],
  "contributes": {
    "languages": [
      {
        "id": "spinscript",
        "aliases": ["SpinScript", "spinscript"],
        "extensions": [".spin", ".spinscript"],
        "configuration": "./language-configuration.json"
      }
    ],
    "commands": [
      {
        "command": "showpp.openPulseSequenceViewer",
        "title": "Open Pulse Sequence Viewer"
      },
      {
        "command": "showpp.closePulseSequenceViewer",
        "title": "Close Pulse Sequence Viewer"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
  ,
  "dependencies": {
    "fast-xml-parser": "^4.2.4"
  }
}
